{
	"rewrites": [{ "source": "/(.*)", "destination": "/" }],
	"routes": [
		{
			"src": "/images/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=31536000, immutable"
			}
		},
		{
			"src": "/icons/(.*)",
			"headers": {
				"Cache-Control": "public, max-age=31536000, immutable"
			}
		}
	]
}
