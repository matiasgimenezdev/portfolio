{
	"rewrites": [
		{
			"source": "/images/:slug*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			],
			"destination": "/images/:slug*"
		},
		{
			"source": "/icons/:slug*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			],
			"destination": "/icons/:slug*"
		},
		{ "source": "/(.*)", "destination": "/" }
	]
}
